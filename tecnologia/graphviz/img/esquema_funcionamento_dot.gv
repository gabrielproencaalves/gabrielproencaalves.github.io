digraph {
  node [shape="note"]

  dotgv -> DOT -> {
    dotjpg
    dotpng
    dotpdf
    dotbmp
    dotgif
    dotsvg
  }

  DOT [shape="none"]

  dotgv  [label=".gv"]
  dotjpg [label=".jpg"]
  dotpng [label=".png"]
  dotpdf [label=".pdf"]
  dotbmp [label=".bmp"]
  dotgif [label=".gif"]
  dotsvg [label=".svg"]
}
